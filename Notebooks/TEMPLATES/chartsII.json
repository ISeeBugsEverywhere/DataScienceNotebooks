	"Warnings": {
		"scope": "python,jupyter",
		"prefix": ":warnings",
		"body": [
			"import warnings",
			"warnings.filterwarnings('ignore')"
		],
		"description": "Disable Warnings"
	},
	"Streamlit":{
		"scope":"python",
		"prefix":":streamlit",
		"body":[
			"import streamlit as st",
			"import pandas as pd",
			"import numpy as np",
			"import plotly.express as px",
			"import mysql.connector as cnt",
			"import matplotlib.pyplot as plt",
			"#streamlit page config:",
			"st.set_page_config(page_icon=':bar_chart', page_title='DEMO STREAMLIT MAP', layout='centered')"
		],
		"description":"Streamlit template"
	},
	"BARS2": {
		"scope": "python,jupyter",
		"prefix": ":bar2",
		"body": [
			"x=np.arange(0, len(?)) # ? - list with labels",
			"w = 0.25",
			"?.bar(x-w/2, ?, label=?, color='xkcd:indigo', width=w)",
			"?.bar(x+w/2, ?, label=?, color='xkcd:deep orange', width=w)",
			"?.set_xticks(x)",
			"?.set_xticklabels(?)"
		],
		"description": "Bar charts (2)"
	},
	"BARS3": {
		"scope": "python,jupyter",
		"prefix": ":bar3",
		"body": [
			"x=np.arange(0, len(?)) # ? - list with labels",
			"w = 0.25",
			"?.bar(x-w, ?,  label=?, color='xkcd:indigo', width=w)",
			"?.bar(x, ?,  label=?, color='xkcd:deep orange', width=w)",
			"?.bar(x+w, ?,  label=?, color='xkcd:deep blue', width=w)",
			"?.set_xticks(x)",
			"?.set_xticklabels(?)"
		],
		"description": "Bar charts (3)"
	},
	"BARS": {
		"scope": "python,jupyter",
